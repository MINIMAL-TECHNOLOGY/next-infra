{
  "version": "0.0.1",
  "name": "@mt/next-infra",
  "description": "Next JS Infrastructure",
  "keywords": [
    "next",
    "nextjs",
    "infrastructure"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "18 || 20"
  },
  "scripts": {
    "clean": "rm -rf ./dist ./tsconfig.tsbuildinfo",
